    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>NugetBougiesSVR</PackageId>
    <Title>Nuget Bougies</Title>
    <Authors>Sofia Villarejo</Authors>
    <Description>Nuget para mi proyecto web de Velas Bougies</Description>
    <PackageIcon>Bougies.png</PackageIcon>
    <PackageTags>NET, nuget, C#, velas, clase</PackageTags>
